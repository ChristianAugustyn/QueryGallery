          android:versionCode="1"
          android:versionName="1.0">
    <meta-data android:name="android.app.searchable"
        android:resource="@xml/searchable" />
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-feature android:name="android.hardware.camera"/>

    <application
        android:allowBackup="false"
        android:icon="@drawable/icon"
        android:label="@string/app_name">
        <activity
            android:name="ca.yorku.eecs.mack.democamera71179.DemoCamera71179Activity"
            android:configChanges="orientation|screenSize"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="ca.yorku.eecs.mack.democamera71179.ImageGridViewActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
        </activity>
        <activity android:name="ca.yorku.eecs.mack.democamera71179.ImageViewerActivity">
        </activity>
